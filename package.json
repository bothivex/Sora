{
  "name": "sora",
  "private": true,
  "sideEffects": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:worker": "remix-pwa build",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "clean": "rimraf .cache ./public/build ./public/sw.js ./build",
    "dev": "run-p \"dev:*\"",
    "dev:worker": "remix-pwa dev",
    "dev:remix": "cross-env NODE_ENV=development remix dev --manual",
    "format-code": "pnpm run prettier:fix & pnpm run lint:fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prebuild": "pnpm cleanup",
    "prettier": "prettier app --check",
    "prettier:fix": "prettier app --write",
    "start": "dotenv -e .env remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fontsource/sora": "^5.0.14",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@nasa-gcn/remix-seo": "^2.0.0",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@nextui-org/accordion": "^2.0.23",
    "@nextui-org/avatar": "^2.0.21",
    "@nextui-org/badge": "^2.0.19",
    "@nextui-org/button": "^2.0.21",
    "@nextui-org/card": "^2.0.21",
    "@nextui-org/checkbox": "^2.0.22",
    "@nextui-org/chip": "^2.0.21",
    "@nextui-org/divider": "^2.0.20",
    "@nextui-org/image": "^2.0.21",
    "@nextui-org/input": "^2.1.9",
    "@nextui-org/kbd": "^2.0.20",
    "@nextui-org/link": "^2.0.22",
    "@nextui-org/pagination": "^2.0.22",
    "@nextui-org/progress": "^2.0.21",
    "@nextui-org/shared-icons": "^2.0.4",
    "@nextui-org/skeleton": "^2.0.19",
    "@nextui-org/spacer": "^2.0.19",
    "@nextui-org/spinner": "^2.0.19",
    "@nextui-org/switch": "^2.0.21",
    "@nextui-org/system": "^2.0.10",
    "@nextui-org/theme": "^2.1.9",
    "@nextui-org/tooltip": "^2.0.24"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@remix-run/serve": "^2.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,json,css,md}": "pnpm run prettier",
    "*.{tsx,ts,jsx,js}": "pnpm run lint"
  },
  "engines": {
    "node": ">=18"
  }
}
